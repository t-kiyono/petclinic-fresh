{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "prisma:gen": "deno run -A --unstable npm:prisma generate --data-proxy",
    "esm:add": "deno run -A https://esm.sh/v116 add",
    "esm:update": "deno run -A https://esm.sh/v116 update",
    "esm:remove": "deno run -A https://esm.sh/v116 remove"
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}